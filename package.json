{
  "name": "gha-create-asana-task",
  "version": "1.0.0",
  "description": "Create Asana task Github Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/gha-create-asana-task.git"
  },
  "keywords": [
    "actions",
    "asana",
    "github"
  ],
  "author": "Honeycomb",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "asana": "^1.0.0",
    "axios": "^1.2.0"
  },
  "devDependencies": {
    "@types/asana": "^0.18.11",
    "@types/node": "^18.11.10",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.0",
    "typescript": "^4.9.3"
  }
}
